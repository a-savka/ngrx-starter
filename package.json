    "string-replace-loader": "^1.2.0",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.2",
    "tslib": "^1.6.1",
    "tslint": "~4.5.1",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.7.2",
    "webpack": "~2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "~4.1.0"
  }
}
